{
    "name": "alpha/guzzle-with-logger",
    "description": "Guzzle with Logger",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpha Olomi", 
            "role": "Developer",
            "email": "alphaolomi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "gmponos/guzzle_logger": "^2.2",
        "guzzlehttp/guzzle": "^7.7",
        "monolog/monolog": "^3.4",
        "symfony/console": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Alpha\\Guzzle\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alpha\\Guzzle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}
